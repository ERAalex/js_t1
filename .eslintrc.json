{
    "env" : {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends" : "airbnb-base",
    "parserOptions": {
        "ecmaVersions": 2018,
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "semi": 0,
        "no-console": "off",
        "no-unused-vars": "off",
        "max-classes-per-file": "off"
     }
}
